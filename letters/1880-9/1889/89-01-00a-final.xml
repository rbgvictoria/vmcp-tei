                        <seg rend="DefaultParagraphFont">
                           <space dim="horizontal" extent="tab"/>
                        </seg>
                        <seg rend="DefaultParagraphFont">Exposition universelle, Paris, 1889.<space unit="chars" quantity="1"/>
                        </seg>
