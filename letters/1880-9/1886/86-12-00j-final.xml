               <seg rend="DefaultParagraphFont" style="">i</seg>
               <seg rend="DefaultParagraphFont" style="">me</seg>
               <seg rend="DefaultParagraphFont" style="">ns<space unit="chars" quantity="1"/>
               </seg>
               <seg rend="DefaultParagraphFont" style="">before pronouncing a fi</seg>
               <seg rend="DefaultParagraphFont" style="">na</seg>
               <seg rend="DefaultParagraphFont" style="">l<space unit="chars" quantity="1"/>
               </seg>
               <seg rend="DefaultParagraphFont" style="">decision.]</seg>
