                     <p rend="letter"
                        style="font-size: 12pt; hyphenate: false; text-align: end; "
                        xml:lang="en">£</p>
                  </cell>
                  <cell style="border-top: none; border-left: none; border-bottom: 0.0069in solid #000000; border-right: none; padding-top: 0in; padding-left: 0.075in; padding-bottom: 0in; padding-right: 0.075in; ">
                     <p rend="letter"
                        style="font-size: 12pt; hyphenate: false; text-align: end; "
                        xml:lang="en">1.</p>
                  </cell>
                  <cell style="border-top: none; border-left: none; border-bottom: 0.0069in solid #000000; border-right: none; padding-top: 0in; padding-left: 0.075in; padding-bottom: 0in; padding-right: 0.075in; ">
                     <p rend="letter"
                        style="font-size: 12pt; hyphenate: false; text-align: end; "
                        xml:lang="en">1</p>
                  </cell>
                  <cell style="border: none; padding-top: 0in; padding-left: 0.075in; padding-bottom: 0in; padding-right: 0.075in; ">
                     <space dim="vertical" unit="lines" quantity="1"/>
                  </cell>
