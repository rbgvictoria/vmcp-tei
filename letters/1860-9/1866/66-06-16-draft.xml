            <p rend="plant%20names" xml:lang="en">
               <name key="Olea">Olea</name>
            </p>
            <p rend="plant%20names" xml:lang="en">
               <name key="Pittosporum">Pittosporum</name>
            </p>
            <p rend="plant%20names" xml:lang="en">
               <name key="Platanus">Platanus</name>
            </p>
            <p rend="plant%20names" xml:lang="en">
               <name key="Viola">Viola</name>
            </p>
