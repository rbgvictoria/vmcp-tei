            <space dim="vertical" unit="lines" quantity="1"/>
            <p rend="letter"
               style="font-size: 12pt; hyphenate: false; "
               xml:lang="en">
               <seg rend="formtext" style="color: #2F5496; ">Read to the Society on the</seg>
               <space unit="chars" quantity="1"/>7th<space unit="chars" quantity="1"/>
               <seg rend="formtext" style="color: #2F5496; ">day of</seg>
               <space unit="chars" quantity="1"/>November<space unit="chars" quantity="1"/>
