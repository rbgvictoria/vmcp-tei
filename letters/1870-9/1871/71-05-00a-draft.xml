                        <seg rend="formtext" style="color: #2F5496; font-variant: small-caps; ">ubdiv</seg>
                        <seg rend="formtext" style="color: #2F5496; ">. No.</seg>
                        <space unit="chars" quantity="1"/>2 <space unit="chars" quantity="2"/>
                        <seg rend="formtext" style="color: #2F5496; font-variant: small-caps; ">Year</seg>
                        <seg rend="formtext" style="color: #2F5496; ">
                           <space unit="chars" quantity="1"/>187</seg>1.</p>
                     <p rend="letter"
                        style="font-size: 12pt; hyphenate: false; "
                        xml:lang="en">
                        <seg rend="formtext" style="color: #2F5496; ">Item</seg>: <space unit="chars" quantity="10"/>Incidentals</p>
                  </cell>
