               <row>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Bamboo seeds</p>
                  </cell>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Systematic Name</p>
                  </cell>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Burmese name</p>
                  </cell>
               </row>
               <row>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">1 Packet from Upper Burma</p>
                  </cell>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">
                        <name key="Dendrocalamus%20Brandisii">Dendrocalamus Brandisii</name>
                     </p>
                  </cell>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Wa-bo.</p>
                  </cell>
               </row>
               <row>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Do from the Minba District of Upper Burma</p>
                  </cell>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Not known.</p>
                  </cell>
                  <cell style="padding-left: 0.075in; padding-right: 0.075in; padding-top: 0in; padding-bottom: 0in; border: none; ">
                     <p rend="letter"
                        style="font-family: Geneva; font-size: 12pt; "
                        xml:lang="en">Thanet-wa</p>
                  </cell>
               </row>
            </table>
            <space dim="vertical" unit="lines" quantity="1"/>
            <space dim="vertical" unit="lines" quantity="1"/>
            <space dim="vertical" unit="lines" quantity="1"/>
