            <p rend="letter"
               style="font-family: Geneva, Geneva; font-size: 12pt; "
               xml:lang="en">90.<space dim="horizontal" extent="tab"/>
               <seg style="background-color: #ffff00; ">Roeleria</seg>
               <seg style="background-color: #ffff00; ">
                  <note xml:id="ftn4" type="footnote" n="4">
